#include<stdio.h>
#include<iostream.h>

void main()
{
  int a[15][15], b[15][15], x1, x2, y1, y2, i, j;
  printf("Enter max power of x and y respectively in first polynomial\n");
  scanf("%d %d", &x1, &y1);
  printf("Enter max power of x and y respectively in second polynomial\n");
  scanf("%d %d", x2, y2);
  if (x1>x2)
    {
      for (i=x2; i<x1; i++)
	{
	  b[i][] = 0;
	}
    }
  else
    {
      for (i=x1; i<x2; i++)
	{
	  a[i][] = 0;
	}
    }